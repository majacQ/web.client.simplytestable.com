<ul class="refs">
    {% for ref,errors in errors_by_ref %}
        <li>
            <h3 class="col-lg-offset-1">
                <a href="{{ ref|escape('html') }}">
                    <i class="fa fa-file-code-o"></i>{{ (ref is empty) ? task.url : ref }}
                </a>
            </h3>

            <ul class="issues css-validation-issues">
                {% for message in errors %}
                    <li>
                        {% include [
                        'SimplyTestableWebClientBundle:bs3/Partials/Test/Results/Task/Succeeded/ErrorList/css-validation:error.html.twig'
                        ]|join('') %}
                    </li>
                {% endfor %}
            </ul>
        </li>
    {% endfor %}
</ul>